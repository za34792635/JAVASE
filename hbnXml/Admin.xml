<?xml version="1.0" encoding="UTF-8"?>

<hibernate-mapping package="com.qyspeed.domain">
    
    <class name="Admin" table="admin">
        <id name="username" type="string" length="20">
            <generator class="assigned">
         </generator>
         </id>
         
        <property
             name="password"
             column="password"
             type="string"
             length="20" 
             />
         </class>
         
    </hibernate-mapping>>